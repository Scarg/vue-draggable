{
  "name": "vue-swipeable",
  "version": "1.0.3",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "tslint -p tsconfig.json",
    "serve-demo": "vue-cli-service serve ./demo/src/main.js",
    "build-demo": "vue-cli-service build demo/src/main.js --dest ./demo/dist",
    "deploy-demo": "gh-pages-deploy"
  },
  "gh-pages-deploy": {
    "staticpath": "demo/dist",
    "prep": [
      "build-demo"
    ],
    "noprompt": true,
    "commit": "gh-deploy-test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Scarg/vue-swipeable.git"
  },
  "keywords": [
    "vue",
    "typescript",
    "swipe",
    "drag",
    "directive",
    "draggable",
    "swipeable",
    "pull",
    "pullable",
    "notification",
    "action",
    "refresh"
  ],
  "author": "Artem Martynyuk aka Scarg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Scarg/vue-swipeable/issues"
  },
  "homepage": "https://github.fcom/Scarg/vue-swipeable#readme",
  "devDependencies": {
    "@types/vue": "^2.0.0",
    "@vue/cli-service": "^4.1.2",
    "gh-pages-deploy": "^0.5.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "tslint -p tsconfig.json",
      "git add"
    ]
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {}
}
